{
  "name": "angular-starter-project",
  "version": "0.0.0",
  "scripts": {
    "dev-web": "ng serve --proxy-config proxy.conf.json --open",
    "dev-api": "tsx watch src/server/",
    "dev": "concurrently \"npm:dev-api\" \"npm:dev-web\" -k -n \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\" ",
    "ng": "ng",
    "start": "tsx src/server/",
    "build": "ng build && tsc -p tsconfig.server.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.11",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.3.11",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/router": "^17.3.11",
    "@types/googlemaps": "^3.43.3",
    "@types/node-fetch": "^2.6.6",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.18.1",
    "googlemaps": "^1.12.0",
    "helmet": "^5.1.1",
    "heroku-ssl-redirect": "^0.1.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.7.0",
    "password-hash": "^1.2.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.12",
    "remult": "^0.26.13",
    "rxjs": "~7.5.0",
    "tsx": "^3.13.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.11",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.10.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.11.1",
    "@types/password-hash": "^1.2.20",
    "@types/pg": "^8.6.5",
    "@types/xlsx": "^0.0.36",
    "concurrently": "^7.2.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.4.5"
  },
  "browser": {
    "jsonwebtoken": false,
    "password-hash": false,
    "node-fetch": false
  }
}
